{
  "name": "@keystatic-starter-docs/repo",
  "version": "0.1.0",
  "scripts": {
    "dev": "pnpm --filter @keystatic-starter-docs/docs dev",
    "build:docs": "pnpm --filter @keystatic-starter-docs/docs build",
    "build:packages": "preconstruct build",
    "start": "pnpm --filter @keystatic-starter-docs/docs start",
    "lint": "pnpm --filter @keystatic-starter-docs/docs lint",
    "postinstall": "preconstruct dev"
  },
  "private": true,
  "preconstruct": {
    "packages": [
      "packages/*"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.21.5",
    "@babel/plugin-transform-runtime": "^7.21.4",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.5",
    "@preconstruct/cli": "^2.5.0",
    "typescript": "4.9.5"
  }
}
